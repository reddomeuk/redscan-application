{
  "name": "@redscan/ai-assistant",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "vite",
    "dev:server": "nodemon server/index.js",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build",
    "build:server": "node scripts/build-server.js",
    "start": "node server/index.js",
    "test": "vitest",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "compression": "^1.7.4",
    "rate-limiter-flexible": "^3.0.8",
    "winston": "^3.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@redscan/shared-ui": "workspace:*",
    "@redscan/shared-auth": "workspace:*"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "vite": "^4.4.5",
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.0",
    "eslint": "^8.45.0",
    "vitest": "^0.34.0"
  },
  "peerDependencies": {
    "@redscan/shared-ui": "*",
    "@redscan/shared-auth": "*"
  }
}